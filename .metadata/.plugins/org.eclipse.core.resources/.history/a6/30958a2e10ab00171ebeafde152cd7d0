
public class Main {
	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		
		ThreadDemo th = new ThreadDemo();
		th.start();
		
		//线程的状态
		//被创建
		th.sleep(1000);//冻结 睡眠
		th.wait(); //冻结 等待
		
		for (int i  = 0; i < 10; i++){
			System.out.println("this is class main" + i);
		}
	}
}
